<!DOCTYPE html>
<html>
    <head>
        <title>Dashboard</title>
        <%- include layout/head %>
    </head>
    <body>
        <%- include layout/nav %>
        <div class="">
            <div class="panel panel-default">
                <div class="panel-heading">
                    User list
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Email</th>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Created Time</th>
                                    <th>Update Time</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (data && data.users) { %>
                                <% for(var i = 0; i < data.users.length; i++){ %>
                                <tr>
                                    <td>
                                        <%= data.users[i].id %>
                                    </td>
                                    <td>
                                        <%= data.users[i].email %>
                                    </td>
                                    <td>
                                        <%- data.users[i].first_name %>
                                    </td>
                                    <td>
                                        <%= data.users[i].last_name %>
                                    </td>
                                    <td>
                                        <%= data.users[i].created_at %>
                                    </td>
                                    <td>
                                        <%= data.users[i].updated_at %>
                                    </td>
                                </tr>
                                <% } %>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
    </body>
</html>